<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="ManageUserRoles">
    <page>/pages/ManageUserRoles.jspx</page>
  </view>
  <view id="ManageStates">
    <page>/pages/ManageStates.jspx</page>
  </view>
  <view id="ManageRoles">
    <page>/pages/ManageRoles.jspx</page>
  </view>
  <view id="ManageRiskPurpose">
    <page>/pages/ManageRiskPurpose.jspx</page>
  </view>
  <view id="ManageCompound">
    <page>/pages/ManageCompound.jspx</page>
  </view>
  <view id="SearchCRS">
    <page>/pages/SearchCRS.jspx</page>
  </view>
  <view id="CrsCreate">
    <page>/pages/CrsCreate.jspx</page>
  </view>
  <view id="Home">
    <page>/pages/Home.jspx</page>
  </view>
  <view id="BrowseSearch">
    <page>/pages/BrowseSearch.jspx</page>
  </view>
  <control-flow-rule id="__2">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__3">
      <from-outcome>userRoles</from-outcome>
      <to-activity-id>ManageUserRoles</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__4">
      <from-outcome>states</from-outcome>
      <to-activity-id>ManageStates</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__5">
      <from-outcome>roles</from-outcome>
      <to-activity-id>ManageRoles</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__6">
      <from-outcome>riskPurpose</from-outcome>
      <to-activity-id>ManageRiskPurpose</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__7">
      <from-outcome>compound</from-outcome>
      <to-activity-id>ManageCompound</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__8">
      <from-outcome>update</from-outcome>
      <to-activity-id>SearchCRS</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__9">
      <from-outcome>create</from-outcome>
      <to-activity-id>CrsCreate</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__10">
      <from-outcome>home</from-outcome>
      <to-activity-id>Home</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__11">
      <from-outcome>browseSearch</from-outcome>
      <to-activity-id>BrowseSearch</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__1">
    <managed-bean-name>sessionBean</managed-bean-name>
    <managed-bean-class>com.novartis.ecrs.view.beans.SessionBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</adfc-config>
