<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="CompoundsTF">
    <default-activity>CreateInsert</default-activity>
    <managed-bean id="__3">
      <managed-bean-name>compoundsBean</managed-bean-name>
      <managed-bean-class>com.novartis.ecrs.ui.bean.CompoundsBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <exception-handler>exceptionHandler</exception-handler>
    <view id="searchCompounds">
      <page>/ui/pageFragments/compounds/searchCompounds.jsff</page>
      <train-stop>
        <display-name>Search</display-name>
      </train-stop>
    </view>
    <view id="compoundDetails">
      <page>/ui/pageFragments/compounds/compoundDetails.jsff</page>
      <train-stop>
        <display-name>Details</display-name>
      </train-stop>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <view id="exceptionHandler">
      <page>/ui/pageFragments/exception/exceptionHandler.jsff</page>
    </view>
    <parent-action id="home">
      <parent-outcome>home</parent-outcome>
    </parent-action>
    <train/>
    <control-flow-rule id="__1">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>searchCompounds</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>searchCompounds</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>home</from-outcome>
        <to-activity-id>home</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
