RISK_PURPOSE_RM_FLAG, 
RISK_PURPOSE_SP_FLAG,
SAFETY_TOPIC_OF_INTEREST, 
SOC_DICT_CONTENT_ENTRY_TS, 
SOC_DICT_CONTENT_ID, 
SOC_TERM, 
CRS_RISK_DEFN_ID,
MEDDRA_CODE, 
MEDDRA_LEVEL, 
MEDDRA_QUALIFIER, 
MEDDRA_TERM, 
MEDDRA_VERSION, 
MEDDRA_VERSION_DATE, 
SEARCH_CRITERIA_DETAILS, 
TMS_DICT_CONTENT_ENTRY_TS, 
TMS_DICT_CONTENT_ID, 
TMS_END_TS, 
TMS_UPDATE_FLAG, 
TMS_UPDATE_FLAG_DT,
DECODE(RISK_PURPOSE_SP_FLAG, 'Y', 'SP,') || DECODE(RISK_PURPOSE_DS_FLAG, 'Y', 'DS,') ||
DECODE(RISK_PURPOSE_RM_FLAG, 'Y', 'RM,') || DECODE(RISK_PURPOSE_PS_FLAG, 'Y', 'PS,') ||
DECODE(RISK_PURPOSE_CD_FLAG, 'Y', 'CD,') || DECODE(RISK_PURPOSE_IB_FLAG, 'Y', 'IB,') ||
DECODE(RISK_PURPOSE_OS_FLAG, 'Y', 'OS,') || DECODE(RISK_PURPOSE_MI_FLAG, 'Y', 'MI,') ||
DECODE(RISK_PURPOSE_ER_FLAG, 'Y', 'ER,') AS RISK_PURPOSE_LIST,
MEDDRA_DICT,
CRS_EFFECTIVE_DT_REL,
CRS_EFFECTIVE_DT_DEF CRS_EFFECTIVE_DT_DEF,
MEDDRA_EXTENSION
FROM CRS_RISK_RELATIONS_STG_FLAT_VW]]></SQLQuery>
