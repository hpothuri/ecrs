<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CRSCountPerMeddraReportVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.novartis.ecrs.model.view.report.CRSCountPerMeddraReportVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT meddra_term meddra_term,
  DECODE(SUBSTR(meddra_level,1,3), 'SMQ','SMQ','NMQ','NMQ','CMQ','CMQ', meddra_level) meddra_comp_defn,
  COUNT(*) number_of_crs
FROM
  (SELECT d.meddra_term,
    DECODE(SUBSTR(d.meddra_level,1,3), 'SMQ','SMQ', 'NMQ','NMQ', 'CMQ','CMQ',NULL) meddra_level,
    c.crs_id crs_id
  FROM crs_compounds p,
    crs_content c,
    crs_risk_relations r,
    crs_risk_definitions d
  WHERE p.compound_id       = c.compound_id
  AND p.compound_type       = 'COMPOUND'
  AND c.release_status_flag = 'C'
  AND c.crs_id              = r.crs_id
  AND r.crs_risk_id         = d.crs_risk_id
  GROUP BY d.meddra_term,
    DECODE(SUBSTR(d.meddra_level,1,3), 'SMQ','SMQ','NMQ','NMQ','CMQ','CMQ',NULL)
  )
GROUP BY meddra_term,
  meddra_level]]></SQLQuery>
  <ViewAttribute
    Name="MeddraTerm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MEDDRA_TERM"
    Expression="MEDDRA_TERM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="300"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="MeddraCompDefn"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="3"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MEDDRA_COMP_DEFN"
    Expression="MEDDRA_COMP_DEFN"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="3"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="NumberOfCrs"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="NUMBER_OF_CRS"
    Expression="NUMBER_OF_CRS"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.novartis.ecrs.model.ECRSModelBundle"/>
  </ResourceBundle>
</ViewObject>
