<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="RiskDefinitionsSafetyTopicReport"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.novartis.ecrs.model.view.report.RiskDefinitionsSafetyTopicReport_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT c.crs_name,
          r.safety_topic_of_interest,
          (SELECT COUNT(*) 
           FROM crs_risk_definitions
           WHERE crs_risk_id = r.crs_risk_id
             AND SUBSTR(meddra_level,1,3) NOT IN ('SMQ','NMQ','CMQ')) meddra_term_count,
          (SELECT COUNT(*) 
           FROM crs_risk_definitions
           WHERE crs_risk_id = r.crs_risk_id
             AND SUBSTR(meddra_level,1,3) = 'SMQ') smq_count,
          (SELECT COUNT(*) 
           FROM crs_risk_definitions
           WHERE crs_risk_id = r.crs_risk_id
             AND SUBSTR(meddra_level,1,3) = 'NMQ') nmq_count,
          (SELECT COUNT(*) 
           FROM crs_risk_definitions
           WHERE crs_risk_id = r.crs_risk_id
             AND SUBSTR(meddra_level,1,3) = 'CMQ') cmq_count,
          (SELECT COUNT(*) 
           FROM crs_risk_relation_purposes_vw
           WHERE crs_risk_id = r.crs_risk_id
            AND risk_purpose_code = 'CD')          adr_count          
    FROM crs_compounds       p,
         crs_content         c,
         crs_risk_relations  r
    WHERE p.compound_id = c.compound_id
      AND p.compound_type = 'COMPOUND'
      AND c.release_status_flag = 'C'
      AND c.crs_id = r.crs_id
    ORDER BY c.crs_name, r.safety_topic_of_interest]]></SQLQuery>
